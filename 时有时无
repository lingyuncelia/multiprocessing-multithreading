import time
from lxml import etree
import requests
def open_url(path):
    url="http://www.youdao.com/w/{}/#keyfrom=dict2.top".format(path)    
    res = requests.get(url)
    time.sleep(1)
    r=res.text
    selector = etree.HTML(r)
    x='//*[@id="phrsListTab"]/div[2]/ul/li[1]/text()'
    a=selector.xpath(x)
    print(a)
open_url('hand')
open_url('some')
open_url('handsome')
open_url('beautiful')

